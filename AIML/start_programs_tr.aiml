<aiml version="1.0.1" encoding="UTF-8">
	
 <!-- Launching programs -->

    <category>
        <pattern>Firefox'u aç</pattern>
         <template>
             <random>
               <li>Hemen açıyorum efendim</li>
               <li>Tabii ki</li>
               <li>Açılıyor</li>
            </random>
            <system>"C:\Program Files\Firefox Developer Edition\firefox.exe"</system>
        </template>
    </category>

    <category>
        <pattern>Firefox uygulamasını aç</pattern>
         <template>
             <random>
               <li>Hemen açıyorum efendim</li>
               <li>Tabii ki</li>
               <li>Açılıyor</li>
            </random>
            <system>"C:\Program Files\Firefox Developer Edition\firefox.exe"</system>
        </template>
    </category>

