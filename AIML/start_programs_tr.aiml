    <category>
        <pattern>Haritalar *</pattern>
        <template>
             <random>
               <li>İstediğiniz konumu haritalar üzerinde açıyorum</li>
               <li>Haritalar açılıyor</li>
               <li>Konumu açıyorum.</li>
            </random>
            <system> "C:\Program Files\Firefox Developer Edition\firefox.exe" "https://www.google.co.in/maps/search/<star/>"</system>
        </template>
