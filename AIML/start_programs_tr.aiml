    <category>
        <pattern>GOOGLE *</pattern>
        <template>
             <random>
               <li>Sonuçları listeliyorum </li>
               <li>Arama sonuçları karşınızda efendim</li>
            </random>
            <system> "C:\Program Files\Firefox Developer Edition\firefox.exe" "http://www.google.com/search?q=<star/>"</system>
        </template>
    </category>

    <category>
        <pattern>YOUTUBE *</pattern>
        <template>
             <random>
               <li>Sonuçları listeliyorum </li>
               <li>Videoları listeliyorum</li>
            </random>
            <system> "C:\Program Files\Firefox Developer Edition\firefox.exe" "https://www.youtube.com/results?search_query=<star/>"</system>
        </template>
    </category>
    <category>
        <pattern>BARBIE</pattern>
        <template>
